	pages = {240--256},
	file = {Accepted Version:/Users/adebimpe/Zotero/storage/CETLP2IT/Satterthwaite et al. - 2013 - An improved framework for confound regression and .pdf:application/pdf;ScienceDirect Full Text PDF:/Users/adebimpe/Zotero/storage/CLIZIABN/Satterthwaite et al. - 2013 - An improved framework for confound regression and .pdf:application/pdf;ScienceDirect Snapshot:/Users/adebimpe/Zotero/storage/SMSXEFDU/S1053811912008609.html:text/html},
