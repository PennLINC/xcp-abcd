	pages = {199--204},
	file = {ScienceDirect Full Text PDF:/Users/adebimpe/Zotero/storage/2BJ7UDSV/Zhang et al. - 2019 - Surface-based regional homogeneity in bipolar diso.pdf:application/pdf;ScienceDirect Snapshot:/Users/adebimpe/Zotero/storage/2USZM4J4/S0165178119305384.html:text/html},
